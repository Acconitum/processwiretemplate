{
	"name": "processwiretemplate",
	"version": "1.0.0",
	"description": "A basic template to start developing with processwire, UiKit and webpack",
	"main": "index.js",
	"browserslist": [
		"last 8 versions"
	],
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"babel": {
		"presets": [
			"env"
		]
	},
	"scripts": {
		"build": "./node_modules/.bin/webpack",
		"build:prod": "./node_modules/.bin/webpack -p",
		"watch": "./node_modules/.bin/webpack --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/Acconitum/processwiretemplate.git"
	},
	"keywords": [
		"processwire"
	],
	"author": "Marc Mentha",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Acconitum/processwiretemplate/issues"
	},
	"homepage": "https://github.com/Acconitum/processwiretemplate#readme",
	"devDependencies": {
		"autoprefixer": "^8.6.3",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.4",
		"babel-preset-env": "^1.7.0",
		"clean-webpack-plugin": "^0.1.19",
		"css-loader": "^0.28.11",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"file-loader": "^1.1.11",
		"node-sass": "^4.9.0",
		"postcss-loader": "^2.1.5",
		"sass-loader": "^7.0.3",
		"style-loader": "^0.21.0",
		"webpack": "^4.12.0",
		"webpack-cli": "^3.0.8"
	}
}
